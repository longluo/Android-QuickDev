<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Default screen margins, per the Android Design guidelines. -->
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="fab_margin">16dp</dimen>

    <!-- on devices older than API 19, we don't need to pad top of nav drawer since nav drawer
won't go "under" status bar -->
    <dimen name="nav_drawer_top_padding">0dp</dimen>
    
    <!-- 默认分割线大小 -->
    <dimen name="line_size">1px</dimen>

    <!-- 默认按钮的圆角大小 -->
    <dimen name="button_circle_size">999dp</dimen>

    <!-- 通用对话框圆角大小 -->
    <dimen name="dialog_ui_round_size">15dp</dimen>
</resources>
